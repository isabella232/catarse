- if resource.contributions.empty?
  .w-container
    .w-row.u-margintop-30.u-text-center
      .w-col.w-col-3
      .w-col.w-col-6
        .fontsize-large.u-marginbottom-30 You have not supported any on Startio
        .w-row
          .w-col.w-col-3
          .w-col.w-col-6
            = link_to 'Contribute now!', explore_path(), class: 'btn btn-large'
          .w-col.w-col-3
      .w-col.w-col-3

- else
  - if resource.contribution_details.for_online_projects.present?
    .section-one-column.u-marginbottom-30
      .fontsize-large.fontweight-semibold.u-marginbottom-30.u-text-center Ongoing Projects
      = render 'contribution_row', collection: resource.contribution_details.for_online_projects.ordered

  - if resource.contribution_details.for_successful_projects.present?
    .section-one-column.u-marginbottom-30
      .fontsize-large.fontweight-semibold.u-marginbottom-30.u-text-center Successful Projects
      = render 'contribution_row', collection: resource.contribution_details.for_successful_projects.ordered


  - if resource.contribution_details.for_failed_projects.present?
    .section-one-column.u-margintop-30
      .fontsize-large.fontweight-semibold.u-marginbottom-30.u-text-center Non-funded Projects
      = render 'contribution_row', collection: resource.contribution_details.for_failed_projects.ordered
